@model List<Adventure>

    <h1 class="w3-jumbo w3-center w3-animate-top">ALLA</h1>
    <hr class="w3-border-green" style="margin:auto;width:40%">
    <br />  

    <l class="w3-center">
        @foreach (Adventure x in Model)
        {
            <li><a asp-action="getbyid" asp-route-id=@x.Id>@x.Name</a></li>
        }
    </l>


    <br />  
    <p class="w3-large w3-center">
        <a asp-controller="Home" asp-action="index">Tillbaka</a>
    </p>

